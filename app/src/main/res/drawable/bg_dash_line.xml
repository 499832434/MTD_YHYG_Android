<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
            android:bottom="-1dp"
            android:left="-1dp"
            android:right="-1dp"
            android:top="0dp">
        <shape android:shape="rectangle">
            <stroke
                    android:width="1dp"
                    android:color="#dfdfdf"
                    android:dashGap="2dp"
                    android:dashWidth="1dp"/>
            <solid android:color="#f2f2f2"/>
            <padding
                    android:bottom="1dp"
                    android:left="1dp"
                    android:right="1dp"
                    android:top="1dp"/>
        </shape>
    </item>
</layer-list>